{
    "name": "siteEdit",
    "version": "0.1.0",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "author": "Mingming Li",
    "module": "dist/siteedit.esm.js",
    "size-limit": [
        {
            "path": "dist/siteedit.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/siteedit.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@measured/puck": "^0.19.3",
        "@size-limit/preset-small-lib": "^11.2.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "husky": "^9.1.7",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "size-limit": "^11.2.0",
        "tsdx": "^0.14.1",
        "tslib": "^2.8.1",
        "typescript": "^3.9.10",
        "react-slideshow-image": "^4.3.2"
    }
}